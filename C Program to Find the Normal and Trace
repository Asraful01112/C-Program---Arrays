#include <stdio.h>
#include <math.h>

int main() {
    int n;
    printf("Enter the order of the square matrix (n x n): ");
    scanf("%d", &n);

    int a[n][n];
    double normal = 0;
    int trace = 0;

    printf("Enter the elements of the matrix:\n");
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &a[i][j]);
        }
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            normal += a[i][j] * a[i][j];  
            if (i == j) {
                trace += a[i][j];   
            }
        }
    }

    normal = sqrt(normal);

    printf("\nTrace of the Matrix = %d\n", trace);
    printf("Normal of the Matrix = %.3f\n", normal);

    return 0;
}
