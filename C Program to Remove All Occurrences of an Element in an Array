#include <stdio.h>

int main() {
    int n, key;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];

    printf("Enter the elements of the array:\n");
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Enter the element to remove: ");
    scanf("%d", &key);

    int newArr[n];  
    int j = 0;

    for (int i = 0; i < n; i++) {
        if (arr[i] != key) {
            newArr[j++] = arr[i];
        }
    }

    printf("Array after removing all occurrences of %d:\n", key);
    for (int i = 0; i < j; i++) {
        printf("%d ", newArr[i]);
    }

    return 0;
}
